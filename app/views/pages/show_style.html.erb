<div>Wesh from Show Style</div>

<% @photos.each do |photo| %>
  <%= image_tag photo.image %>
<% end  %>

<div class="styles_container">
  <div class="styles" style="z-index: 1;">
    <% @photos.each do |photo| %>
      <%= link_to show_photo_path(photo) do %>
        <div class="style_covers" >
          <%= image_tag photo.image %>
        </div>
      <% end %>
    <% end %>
  </div>
</div>
